         <ExecutionAction
            ActionType = "Xcode.IDEStandardExecutionActionsCore.ExecutionActionType.ShellScriptAction">
            <ActionContent
               title = "Run Script"
               scriptText = "# BEAT AD HOC VECTOR&#10;# This script archives the distribution, creates a disk image and notarizes it with Apple.&#10;# For Sparkle, the actual binary has to be notarized separately.&#10;&#10;# Pipeline:&#10;# &#x2022; create and notarize the DMG&#10;# &#x2022; copy the DMG to Dropbox (for now)&#10;# &#x2022; create GitHub release&#10;# &#x2022; generate Sparkle appcast   &#10;&#10;# notarytool credentials.&#10;# AC_PASSWORD is the name of the keychain item created with `notarytool store-credentials`.&#10;# Grant keychain access to Xcode if prompted by Xcode.&#10;AC_PASSWORD=&quot;BeatNotarization&quot;&#10;&#10;# Do all of the work in a subdirectory of /tmp, and use a&#10;# unique ID so that there&apos;s no collision with previous builds.&#10;EXPORT_UUID=`uuidgen`&#10;EXPORT_PATH=&quot;/tmp/$PRODUCT_NAME-$EXPORT_UUID/&quot;&#10;APP_PATH=&quot;$EXPORT_PATH/$PRODUCT_NAME.app&quot;&#10;&#10;DIST_PATH=&quot;/Users/lmp/dropbox/Beat/&quot;&#10;&#10;mkdir -p &quot;$EXPORT_PATH&quot;&#10;&#10;# Xcode doesn&apos;t show run script errors in build log.&#10;exec &gt; &quot;$EXPORT_PATH/Xcode run script.log&quot; 2&gt;&amp;1&#10;&#10;# Use osascript(1) to present notification banners; otherwise&#10;# there&apos;s no progress indication until the script finishes.&#10;/usr/bin/osascript -e &apos;display notification &quot;Exporting application archive&#x2026;&quot; with title &quot;Submitting app for notarization&quot;&apos;&#10;&#10;# Ask xcodebuild(1) to export the app. Use the export options&#10;# from a previous manual export that used a Developer ID.&#10;/usr/bin/xcodebuild -exportArchive -archivePath &quot;$ARCHIVE_PATH&quot; -exportOptionsPlist &quot;$SRCROOT/Distributions/Adhoc/ExportOptions.plist&quot; -exportPath &quot;$EXPORT_PATH&quot;&#10;&#10;osascript -e &apos;display notification &quot;Creating UDIF Disk Image&#x2026;&quot; with title &quot;Submitting app for notarization&quot;&apos;&#10;&#10;# Create DMG file&#10;cd &quot;$EXPORT_PATH/&quot;&#10;create-dmg &quot;$APP_PATH&quot;&#10;&#10;# Get latest file in folder with .dmg extension &#10;DMG_NAME=$(ls -Art *.dmg | tail -n 1)&#10;&#10;# Create a GitHub-compatible DMG name&#10;DMG_PATH=&quot;$EXPORT_PATH/$DMG_NAME&quot;&#10;&#10;# Extract version number for later use&#10;NAME_AND_VERSION=${DMG_NAME%.dmg}&#10;DMG_VERSION=${NAME_AND_VERSION//Beat /}&#10;&#10;osascript -e &apos;display notification &quot;Creating ZIP package&#x2026;&quot; with title &quot;Submitting for notarization&quot;&apos;&#10;&#10;# Create a zip&#10;# To make it compatible with GitHub release URLs, we&apos;ll rename it to use . instead of a space in the file name, eg. &quot;Beat.1.999.9&quot;&#10;ARCHIVE_NAME=${NAME_AND_VERSION//Beat /Beat.}&#10;ZIP_NAME=$ARCHIVE_NAME.zip&#10;ZIP_PATH=&quot;$EXPORT_PATH/$ZIP_NAME&quot;&#10;/usr/bin/ditto -c -k --keepParent &quot;$APP_PATH&quot; &quot;$ZIP_PATH&quot;&#10;&#10;# Create release notes for Sparkle&#10;RELEASE_NOTES_PATH=&quot;$APP_PATH/Contents/Resources/Release notes.md&quot;&#10;RELEASE_NOTES_BASENAME=&quot;$(basename -- $RELEASE_NOTES_PATH)&quot;&#10;RELEASE_NOTES_HTML_PATH=&quot;$DIST_PATH/Update/$ARCHIVE_NAME.html&quot;&#10;# We&apos;ll use a NPM md-&gt;html generator called marked (https://github.com/markedjs/marked)&#10;marked -o &quot;$RELEASE_NOTES_HTML_PATH&quot; -i &quot;$RELEASE_NOTES_PATH&quot;&#10;&#10;osascript -e &apos;display notification &quot;Submitting UDIF Disk Image for notarization&#x2026;&quot; with title &quot;Submitting for notarization&quot;&apos;&#10;&#10;# Submit the finished deliverables for notarization.&#10;# Wait up to 2 hours for a response.&#10;# Use verbose logging in order to file feedback if an error occurs.&#10;&quot;$DEVELOPER_BIN_DIR/notarytool&quot; submit -p &quot;$AC_PASSWORD&quot; --verbose &quot;$DMG_PATH&quot; --wait --timeout 2h --output-format plist &gt; &quot;NotarizationResponse.plist&quot;&#10;&quot;$DEVELOPER_BIN_DIR/notarytool&quot; submit -p &quot;$AC_PASSWORD&quot; --verbose &quot;$ZIP_PATH&quot; --wait --timeout 2h --output-format plist &gt; &quot;NotarizationResponseForZip.plist&quot;&#10;&#10;return_code=$?&#10;&#10;if [ $return_code -eq 0 ]; then&#10;message=`/usr/libexec/PlistBuddy -c &quot;Print :message&quot; &quot;NotarizationResponse.plist&quot;`&#10;status=`/usr/libexec/PlistBuddy -c &quot;Print :status&quot; &quot;NotarizationResponse.plist&quot;`&#10;messageZip=`/usr/libexec/PlistBuddy -c &quot;Print :message&quot; &quot;NotarizationResponseForZip.plist&quot;`&#10;statusZip=`/usr/libexec/PlistBuddy -c &quot;Print :status&quot; &quot;NotarizationResponseForZip.plist&quot;`&#10;else&#10;message=&quot;An Error Occurred.&quot;&#10;status=&quot;Check Xcode log.&quot;&#10;open &quot;$EXPORT_PATH/Xcode run script.log&quot;&#10;fi&#10;&#10;# Show the final status.&#10;osascript -e &quot;on run(argv)&quot; \&#10;-e &apos;display notification item 1 of argv &amp; &quot; : &quot; &amp; item 2 of argv with title &quot;Submitting app for notarization&quot; sound name &quot;Crystal&quot;&apos; \&#10;-e &apos;set text item delimiters to &quot;, &quot;&apos; \&#10;-e &quot;set args to argv as text&quot; \&#10;-e &quot;delay 5&quot; \&#10;-e &quot;end&quot; \&#10;-- &quot;$message - $messageZip&quot; &quot;$status - $statusZip&quot;&#10;&#10;#open &quot;$EXPORT_PATH&quot;&#10;&#10;# Copy the notarized DMG to distribution folder.&#10;# Then open the folder, which also signals completion.&#10;DMG_DESTINATION=&quot;$DIST_PATH/Dist/$DMG_NAME&quot;&#10;ZIP_DESTINATION=&quot;$DIST_PATH/Update/$ZIP_NAME&quot;&#10;GH_TAG=&quot;v$DMG_VERSION&quot;&#10;&#10;mv -f -v &quot;$DMG_PATH&quot; &quot;$DMG_DESTINATION&quot;&#10;mv -f -v &quot;$ZIP_PATH&quot; &quot;$ZIP_DESTINATION&quot;&#10;&#10;# Copy release notes&#10;# cp &quot;$RELEASE_NOTES_PATH&quot; &quot;/Users/lmp/dropbox/Beat/Update/Beat.$DMG_VERSION.html&quot;&#10;&#10;# Generate release for GitHub&#10;cd &quot;/Users/lmp/Koodaus/Beat&quot;&#10;echo &quot;Creating GitHub version with tag $GH_TAG&quot;&#10;gh release create $GH_TAG --title &quot;Beat $DMG_VERSION&quot; --notes-file &quot;$RELEASE_NOTES_PATH&quot; &quot;$DMG_DESTINATION&quot; &quot;$ZIP_DESTINATION&quot;&#10;# Now we&apos;ll need to create a GitHub download link for Sparkle appcast&#10;RELEASE_URL=&quot;https://github.com/lmparppei/Beat/releases/latest/download/&quot;&#10;&#10;# Generate appcast. Don&apos;t create deltas as long as we&apos;re using GitHub for distributing the zip&#10;/Users/lmp/Koodaus/Sparkle/bin/generate_appcast --link &quot;https://www.beat-app.fi&quot; --full-release-notes-url &quot;https://www.beat-app.fi/patch-notes/$DMG_VERSION/&quot; --maximum-deltas 0 --download-url-prefix &quot;$RELEASE_URL&quot; &quot;/Users/lmp/dropbox/Beat/Update/&quot;&#10;&#10;# Open the folders &#10;open &quot;/Users/lmp/dropbox/Beat/Dist/&quot;&#10;# open &quot;/Users/lmp/dropbox/Beat/Update/&quot;&#10;# open &quot;$EXPORT_PATH/Xcode run script.log&quot;&#10;">
